generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE-URL")
}

model User {
  id       String         @id @default(cuid())
  email    String         @unique
  password String
  sessions UserSessions[]
}

model UserSessions {
  id        User   @relation(fields: [userId], references: [id])
  userId    String @unique
  sessionId String @unique
}
